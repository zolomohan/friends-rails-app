<div class="jumbotron">
  <h1 class="display-4"><%= @friend.first_name %> <%= @friend.last_name %></h1>
  <p class="lead"><%= @friend.email %></p>
  <p class="lead"><%= @friend.phone_number %></p>
  <p class="lead"><%= @friend.twitter %></p>
  <hr class="my-4">
  <p class="lead">
    <%= link_to 'Edit', edit_friend_path(@friend), class:"btn btn-dark" %>
    <%= link_to 'Back', friends_path, class:"btn btn-dark" %>
  </p>
</div>


